.cell
  width cellSize
  height cellSize
  margin-right @width*0.1
  border-radius 4px
  margin-bottom @height *0.1
  background white
  box-shadow rgba(0,0,0,0.2) 0 1px 2px 0
  color #333
  font-size 1.2rem
  font-weight bold
  cursor pointer
  transition all .3s ease-in
  position absolute
.empty
  opacity 0

